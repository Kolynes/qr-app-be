{
   "name": "qr-app-be",
   "version": "1.0.0",
   "description": "",
   "main": "index.ts",
   "scripts": {
      "test": "npm run migrate && jest",
      "start": "cd build && node src/index.js",
      "build": "npm run migrate && (tsc||true)",
      "ts-jest": "ts-jest",
      "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
      "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts ",
      "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
      "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts",
      "serve": "nodemon src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/dinkokec/qr-app-be/issues"
   },
   "homepage": "https://github.com/dinkokec/qr-app-be#readme",
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.13",
      "@types/jest": "^27.4.1",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/migrate-mongo": "^8.2.0",
      "@types/mongodb": "^3.6.4",
      "@types/multer": "^1.4.7",
      "@types/mustache": "^4.1.2",
      "@types/node": "^8.0.29",
      "@types/qrcode": "^1.4.2",
      "@types/supertest": "^2.0.12",
      "jest": "^27.0.0",
      "nodemon": "^2.0.15",
      "supertest": "^6.2.3",
      "ts-jest": "^27.1.4",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/mailgun-js": "^0.22.12",
      "bcrypt": "^5.0.1",
      "body-parser": "^1.19.2",
      "class-mixins": "^0.1.3",
      "dotenv": "^16.0.0",
      "express": "^4.17.3",
      "form-data": "^4.0.0",
      "jsonwebtoken": "^8.5.1",
      "mailgun": "^0.5.0",
      "mailgun-js": "^0.22.0",
      "mailgun.js": "^5.1.0",
      "migrate": "^1.8.0",
      "migrate-mongo": "^9.0.0",
      "mongodb": "^3.6.4",
